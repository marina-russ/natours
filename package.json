    "eslint-plugin-react": "^7.32.2",
    "ndb": "^1.1.5",
    "prettier": "^2.8.8",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "plugins": [
    "import"
  ],
  "rules": {
    "import/no-unresolved": 2,
    "import/no-commonjs": 2,
    "import/extensions": [
      2,
      "ignorePackages"
    ]
  },
  "engines": {
    "node": ">=10.0.0"
  }
