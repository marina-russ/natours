    "url": "https://github.com/marina-russ/natours/issues"
  },
  "homepage": "https://github.com/marina-russ/natours#readme",
  "dependencies": {
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.1.4",
    "esbuild": "0.19.3",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.9.0",
    "helmet": "^7.0.0",
    "helmet-csp": "^3.4.0",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
